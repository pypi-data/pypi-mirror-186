  <h3 i18n:translate="">
      Faculties
  </h3>
  <table class="table table-hover" i18n:domain="waeup.kofa">
    <thead>
      <tr>
        <th i18n:translate="">Code</th>
        <th i18n:translate="">Title</th>
      </tr>
    </thead>
    <tbody>
      <tr tal:repeat="value context/values">
        <td> <a tal:attributes="href python: view.url(value)">
            <span tal:content="value/code">Name</span></a></td>
        <td tal:content="value/longtitle">Title</td>
      </tr>
    </tbody>
  </table>
</tal:so>