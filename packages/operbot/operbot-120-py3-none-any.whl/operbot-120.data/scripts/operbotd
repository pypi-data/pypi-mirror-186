"write your own commands"


import inspect


from opr.handler import Command, scan
from opr.objects import Wd
from opr.runtime import wait



from operbot import cmd, irc, rss


Wd.workdir = "/var/lib/operbot/"


def main():
    scan(cmd)
    scan(irc)
    scan(rss)
    irc.init()
    rss.init()
    wait()


main()
