from typing import Any

class Flash:
    def ioctl(self, *args) -> Any: ...
    def readblocks(self, *args) -> Any: ...
    def writeblocks(self, *args) -> Any: ...

class PIO:
    IN_HIGH: int
    IN_LOW: int
    IRQ_SM0: int
    IRQ_SM1: int
    IRQ_SM2: int
    IRQ_SM3: int
    JOIN_NONE: int
    JOIN_RX: int
    JOIN_TX: int
    OUT_HIGH: int
    OUT_LOW: int
    SHIFT_LEFT: int
    SHIFT_RIGHT: int
