    def irq(self, *args) -> Any: ...
    def remove_program(self, *args) -> Any: ...
    def state_machine(self, *args) -> Any: ...

class StateMachine:
    def exec(self, *args) -> Any: ...
    def get(self, *args) -> Any: ...
    def active(self, *args) -> Any: ...
    def init(self, *args) -> Any: ...
